<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/categoria"
        android:id="@+id/textView14"
        android:layout_marginTop="29dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton7"
        android:background="@drawable/cultura"
        android:layout_marginTop="45dp"
        android:layout_below="@+id/textView14"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton8"
        android:background="@drawable/gastronomia"
        android:layout_above="@+id/checkBox8"
        android:layout_alignRight="@+id/textView14"
        android:layout_alignEnd="@+id/textView14" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton9"
        android:background="@drawable/monumentos"
        android:layout_above="@+id/checkBox8"
        android:layout_alignLeft="@+id/textView14"
        android:layout_alignStart="@+id/textView14" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/categoriaMonumento"
        android:id="@+id/checkBox7"
        android:layout_below="@+id/imageButton9"
        android:layout_toLeftOf="@+id/checkBox8"
        android:layout_toStartOf="@+id/checkBox8" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/categoriaCultura"
        android:id="@+id/checkBox8"
        android:layout_below="@+id/imageButton7"
        android:layout_centerHorizontal="true" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/categoriaGastronomia"
        android:id="@+id/checkBox9"
        android:layout_below="@+id/imageButton8"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton16"
        android:background="@drawable/alojamento"
        android:layout_below="@+id/checkBox7"
        android:layout_alignLeft="@+id/imageButton9"
        android:layout_alignStart="@+id/imageButton9"
        android:layout_marginTop="22dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton17"
        android:background="@drawable/agenda"
        android:layout_alignTop="@+id/imageButton16"
        android:layout_alignLeft="@+id/imageButton7"
        android:layout_alignStart="@+id/imageButton7" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton18"
        android:background="@drawable/praia"
        android:layout_alignTop="@+id/imageButton17"
        android:layout_alignLeft="@+id/imageButton8"
        android:layout_alignStart="@+id/imageButton8" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/categoriaAlojamento"
        android:id="@+id/checkBox10"
        android:layout_below="@+id/imageButton16"
        android:layout_alignLeft="@+id/checkBox7"
        android:layout_alignStart="@+id/checkBox7" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/categoriaAgenda"
        android:id="@+id/checkBox11"
        android:layout_below="@+id/imageButton17"
        android:layout_alignLeft="@+id/checkBox8"
        android:layout_alignStart="@+id/checkBox8"
        android:layout_toStartOf="@+id/imageButton18"
        android:layout_toLeftOf="@+id/imageButton18" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/categoriaPraia"
        android:id="@+id/checkBox12"
        android:layout_below="@+id/imageButton18"
        android:layout_alignLeft="@+id/checkBox9"
        android:layout_alignStart="@+id/checkBox9" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton19"
        android:layout_below="@+id/checkBox11"
        android:layout_alignLeft="@+id/imageButton16"
        android:layout_alignStart="@+id/imageButton16"
        android:layout_marginTop="22dp"
        android:background="@drawable/categoria" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton20"
        android:layout_alignTop="@+id/imageButton19"
        android:layout_alignLeft="@+id/imageButton17"
        android:layout_alignStart="@+id/imageButton17"
        android:background="@drawable/espaconocturno" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton21"
        android:layout_alignBottom="@+id/imageButton20"
        android:layout_alignLeft="@+id/imageButton18"
        android:layout_alignStart="@+id/imageButton18"
        android:background="@drawable/outro" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/categoriaDesporto"
        android:id="@+id/checkBox13"
        android:layout_below="@+id/imageButton19"
        android:layout_alignLeft="@+id/checkBox10"
        android:layout_alignStart="@+id/checkBox10"
        android:layout_toLeftOf="@+id/checkBox14"
        android:layout_toStartOf="@+id/checkBox14" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/categoriaEspaco"
        android:id="@+id/checkBox14"
        android:layout_below="@+id/imageButton20"
        android:layout_alignLeft="@+id/checkBox11"
        android:layout_alignStart="@+id/checkBox11"
        android:layout_toLeftOf="@+id/imageButton21"
        android:layout_toStartOf="@+id/imageButton21" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/categoriaOutro"
        android:id="@+id/checkBox15"
        android:layout_below="@+id/imageButton21"
        android:layout_toRightOf="@+id/checkBox11"
        android:layout_toEndOf="@+id/checkBox11" />

    <SeekBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/seekBar"
        android:layout_below="@+id/checkBox14"
        android:layout_alignLeft="@+id/imageButton20"
        android:layout_alignStart="@+id/imageButton20"
        android:layout_marginTop="35dp"
        android:layout_alignRight="@+id/imageButton21"
        android:layout_alignEnd="@+id/imageButton21" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/defineRaio"
        android:id="@+id/textView15"
        android:layout_alignBottom="@+id/seekBar"
        android:layout_alignRight="@+id/imageButton19"
        android:layout_alignEnd="@+id/imageButton19"
        android:layout_alignTop="@+id/seekBar"
        android:layout_alignLeft="@+id/checkBox13"
        android:layout_alignStart="@+id/checkBox13" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/inicio"
        android:id="@+id/button"
        android:layout_below="@+id/seekBar"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"/>


</RelativeLayout>